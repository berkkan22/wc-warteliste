{
  "name": "wcwarteliste",
  "version": "1.1.0",
  "description": "Toilettenliste",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-win64": "electron-packager . --overwrite --platform=win32  --arch=x64",
    "package-win32": "electron-packager . --overwrite --platform=win32  --arch=ia32",
    "package-linux64": "electron-packager . --overwrite --platform=linux --arch=x64",
    "package-linux32": "electron-packager . --overwrite --platform=linux --arch=ia32",

    "asarPack": "asar pack build/wcwarteliste-linux-ia32/resources/app build/wcwarteliste-linux-ia32/resources/app.asar"
  },
  "build": {
    "productName": "Toilettenliste"
  },
  "author": "Berkkan Katirci",
  "license": "MIT",
  "devDependencies": {
    "asar": "^1.0.0",
    "electron": "^4.0.4",
    "electron-packager": "^13.0.1"
  },
  "dependencies": {
    "electron-winstaller": "^2.7.0",
    "typed.js": "^2.0.10"
  }
}
